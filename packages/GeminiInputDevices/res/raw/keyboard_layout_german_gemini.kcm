#
# German keyboard layout, QWERTZ style.
#

type OVERLAY

map key 12 SLASH            # ï¿½ ? \
#map key 21 Z
#map key 44 Y
map key 53 MINUS            # - _
map key 86 PLUS             # < > |

### ROW 1

key ESCAPE {
    base:                               fallback BACK
    alt, meta:                          fallback HOME
    ctrl:                               fallback MENU
}

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    fn:					                '~'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '"'
    alt:                                '\u00b2'
    fn:					                '#'
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift:                              '\u00a7'
    alt:                                '\u00b3'
    fn:				                	'\\'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
    fn:				                	'\u20ac'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
    fn:				                	'<'
}

key 6 {
    label:                              '6'
    base:                               '6'
    shift:                              '&'
    fn:				                	'>'
}

key 7 {
    label:                              '7'
    base:                               '7'
    shift:                              '/'
    alt:                                '{'
    fn:                                 '{'
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '('
    alt:                                '['
    fn:                                 '['
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              ')'
    alt:                                ']'
    fn:                                 ']'
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              '='
    alt:                                '}'
    fn:                                 '}'
}

key DEL {
    base:				                fallback DEL
    shift, capslock:	        		fallback FORWARD_DEL
}

### ROW 2

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
    alt:                             	'@'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
    alt:                                '\u20ac'
}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
    fn:					                '|'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
}

key Y {
    label:                              'Z'
    base:                               'z'
    shift, capslock:                    'Z'
}

key U {
    label:                              'U'
    base:                               'u'
    shift, capslock:                    'U'
    fn:                               	'\u00fc'
    shift+fn:                        	'\u00dc'
}

key I {
    label:                              'I'
    base:                               'i'
    shift, capslock:                    'I'
    fn:				                	'`'
}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
    fn:                               	'\u00f6'
    shift+fn:		            		'\u00d6'
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}

### ROW 3

key TAB {
    label:                              '\t'
    base:                               '\t'
}

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    fn:                               	'\u00e4'
    shift+fn:		                    '\u00c4'
}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
    fn:	                                '\u00df'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
}

key F {
    label:                              'F'
    base:                               'f'
    shift, capslock:                    'F'
}

key G {
    label:                              'G'
    base:                               'g'
    shift, capslock:                    'G'
    fn:				                	'_'
}

key H {
    label:                              'H'
    base:                               'h'
    shift, capslock:                    'H'
}

key J {
    label:                              'J'
    base:                               'j'
    shift, capslock:                    'J'
    fn:				                	'\''

}

key K {
    label:                              'K'
    base:                               'k'
    shift, capslock:                    'K'
    fn:				                   	'-'
}

key L {
    label:                              'L'
    base:                               'l'
    shift, capslock:                    'L'
    fn:				                   	'+'
}

key APOSTROPHE {
    label:                              '?'
    base:                               '?'
    shift, capslock:                    '*'
    fn:				                	'@'
}

### ROW 4

#key SHIFT_LEFT {
#}

key Z {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
    fn:					                '^'
}

key X {
    label:                              'X'
    base:                               'x'
    shift, capslock:                    'X'
    fn:                              	'\u00b0'
}

key C {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
}

key V {
    label:                              'V'
    base:                               'v'
    shift, capslock:                    'V'
}

key B {
    label:                              'B'
    base:                               'b'
    shift, capslock:                    'B'
}

key N {
    label:                              'N'
    base:                               'n'
    shift, capslock:                    'N'
}

key M {
    label:                              'M'
    base:                               'm'
    shift, capslock:                    'M'
    alt:                                '\u00b5'
    fn:                                 '\u00b5'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift:                              ':'
}


key DPAD_UP {
   base:				                fallback DPAD_UP
   fn:				                	fallback PAGE_UP
}

#key SHIFT_RIGHT {
#}

### ROW 5

#key CTRL_LEFT {
#}

#key FUNCTION {
#}

#key ALT_LEFT {
#}

key SPACE {
    label:                              ' '
    base:                               ' '
    alt, meta:                          fallback SEARCH
    ctrl:                               fallback LANGUAGE_SWITCH
}

key COMMA {
    label:                              ','
    base:                               ','
    shift:                              ';'
}

key DPAD_LEFT {
   base:				                fallback DPAD_LEFT
   fn:				                	fallback MOVE_HOME
}


key DPAD_DOWN {
   base:		                		fallback DPAD_DOWN
   fn:			                		fallback PAGE_DOWN
}


key DPAD_RIGHT {
   base:		                		fallback DPAD_RIGHT
   fn:			                		fallback MOVE_END
}